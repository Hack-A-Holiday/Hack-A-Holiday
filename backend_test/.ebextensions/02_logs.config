files:
  "/opt/elasticbeanstalk/tasks/taillogs.d/app-logs.conf":
    mode: "000755"
    owner: root
    group: root
    content: |
      /var/log/nodejs/nodejs.log

container_commands:
  01_set_npm_registry:
    command: "npm config set registry https://registry.npmjs.org/"
  02_clean_npm:
    command: "npm cache clean --force"
